{
    "routes": [
      {
        "route": "/*",
        "serve": "/index.html",
        "statusCode": 200
      }
    ],
    "navigationFallback": {
          "rewrite": "/index.html",
          "exclude": ["/images/*.{png,jpg,gif}", "/css/*"]
    },
    "platformErrorOverrides": [
      {
        "errorType": "NotFound",
        "statusCode": "302",
        "serve": "/Index.html"
      }
      
    ]
  }